<p id="notice"></p>
<div class="container">
   <h4>ログイン</h4>
    <h6 class="customer-sign_in">会員の方はこちらからログイン</h6>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-group row">
      <label class="col-sm-2">
        <%= f.label :"メールアドレス" %>
      </label>
      <div class="col-sm-6">
        <%= f.email_field :email, autofocus: true, autocomplete: "email",class: "form-control", placeholder: "sample@exsample.com"%>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-sm-2">
         <%= f.label :"パスワード" %>
      </label>
      <div class="col-sm-6">
        <%= f.password_field :password, autocomplete: "current-password",class: "form-control" %>
      </div>
    </div>
      <div class="new_submit">
        <%= f.submit "  ログイン  ",class: "btn btn-primary" %>
      </div>
    <% end %>
</div>